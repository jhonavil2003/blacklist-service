{
    "executionRoleArn": "arn:aws:iam::713843767123:role/ecsTaskExecutionRole",
    "containerDefinitions": [
      {
        "portMappings": [
          {
            "hostPort": 5000,
            "protocol": "tcp",
            "containerPort": 5000
          }
        ],

        "image": "<IMAGE1_NAME>",
        "essential": true,
        "name": "Container-blacklist-app",
        "environment": [
                {
                    "name": "RDS_USERNAME",
                    "value": "postgres"
                },
                {
                    "name": "RDS_PASSWORD",
                    "value": "postgres"
                },
                {
                    "name": "RDS_HOSTNAME",
                    "value": "database-blacklist-service.c50ncohlhglo.us-east-1.rds.amazonaws.com"
                },
                {
                    "name": "RDS_PORT",
                    "value": "5432"
                },
                {
                    "name": "RDS_DB_NAME",
                    "value": "postgres"
                }
        ]

      }
    ],

    "requiresCompatibilities": [
      "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "512",
    "memory": "1024",
    "family": "Task-blacklist-app"
  }